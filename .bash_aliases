# alias for bash
alias ..='cd ..'

# alis for ls
alias ll='ls -l'
alias la='ls -A'
alias lla='ls -Al'
alias lal='ls -Al'
#alias l='ls -CF'

# alias for Git
alias g='git'
alias gs='git status'
alias ga='git add'
alias gap='git add -p'
alias gc='git commit'
alias gcm='git commit -m'
alias gch='git checkout'
alias gchb='git checkout -b'
alias gd='git diff'
alias grb='git rebase -i'

# tmux
alias tmux='tmux -2'
